<?xml version="1.0"?>
 
<launch>
   <include file="$(find husky_gazebo)/launch/empty_world.launch">
      <arg name="world_name" value="$(find husky_controller)/worlds/singlePillar.world" />
   </include>
    
   <node name="husky_controller" pkg="husky_controller" type="husky_controller_node" output="screen" >
   <param name="scan_name" type="string" value="/cluster_center" />
   <param name="scan_queue_size" type="int" value="2" />
   <param name="command_velocity_name" type="string" value="/cmd_vel" />
   <param name="command_velocity_queue_size" type="int" value="10" />
   <param name="kd" type="double" value="0.1" />
   <param name="kw" type="double" value="-0.4" />
   <param name="target_distance" type="double" value="2" />
   </node>

   <!--<node name="teleop" pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" output="screen" />-->

   <node pkg="rviz" type="rviz" name="rviz" args="-d $(find husky_controller)/rviz/laser_scan.rviz"/>
   

</launch>

